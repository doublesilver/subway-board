{
  "name": "subway-board-backend",
  "version": "1.0.0",
  "description": "Backend for subway anonymous board",
  "main": "src/index.js",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "npm run migrate:safe && npm run migrate:kakao:safe && node src/index.js",
    "migrate": "node src/db/migrate.js",
    "migrate:safe": "node src/db/migrate.js || echo 'Migration skipped'",
    "migrate:kakao": "node src/db/migrate-kakao.js",
    "migrate:kakao:safe": "node src/db/migrate-kakao.js || echo 'Kakao migration skipped'",
    "cleanup": "node src/db/cleanup.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.0.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "pg": "^8.14.0",
    "playwright": "^1.57.0",
    "socket.io": "^4.8.3",
    "uuid": "^9.0.1",
    "winston": "^3.19.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  }
}
